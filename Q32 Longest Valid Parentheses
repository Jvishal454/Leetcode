class Solution:
    def longestValidParentheses(self, s: str) -> int:
        '''
        stack = [1,2,3,4,5,7] # last index is -1 not 0
        i = 4
        a = i - stack[-1]
        print(a)
        '''
        stack = [-1]
        mx = 0
        for i,p in enumerate(s):
            if p == "(":
                stack.append(i)
            else:
                stack.pop()
                
                if not stack:
                    stack.append(i)
                else:
                    mx = max(mx, i - stack[-1])
        return mx
